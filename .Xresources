Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

! Clipboard
URxvt.perl-ext-common: default,keyboard-select,matcher
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.clipboard.autocopy: true

! Scrollbar
URxvt*scrollTtyKeypress: true
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt.scrollBar:           false
URxvt.saveLines:           4096

! Style
URxvt.internalBorder:      3
URxvt.externalBorder:      0
URxvt.highlightSelection:  true
URxvt.letterSpace: 0
URxvt.lineSpace: 4

! lol
URxvt.loginShell: true
URxvt*termName: rxvt-unicode-256color

! Fonts
URxvt.xftAntialias: true
URxvt.font: xft:Bitstream Vera Sans Mono:style=regular:pixelsize=13:antialias=true,hinting=true, xft:DejaVu Sans Mono:pixelsize=13:antialias=true,hinting=true, xft:Bitstream Vera Sans Mono:pixelsize=13, xft:DejaVu Sans Mono for Powerline:pixelsize=12
URxvt.boldFont:  xft:Bitstream Vera Sans Mono:style=bold:pixelsize=13:antialias=true,hinting=true, xft:DejaVu Sans Mono:pixelsize=13:antialias=true,hinting=true
URxvt.italicFont: xft:Bitstream Vera Sans Mono:pixelsize=13:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=13:antialias=true,hinting=true
URxvt.boldItalicFont: xft:Bitstream Vera Sans Mono:pixelsize=13:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=13:antialias=true,hinting=true

! URL
URxvt.url-launcher: firefox
URxvt.matcher.button: 1
URxvt*underlineColor: yellow
URxvt.depth: 32

! ?
Urxvt.secondaryScroll:  true

! Appareance
URxvt.transparent:true
URxvt.shading: 14

